<!DOCTYPE html>
<html>
  <head>
    <title>EDA Phase 1 Express Boilerplate</title>
  </head>
  <body>
    <ul>
    {{#each users}}
      <li><a href = "/user/{{id}}">{{name}}</a> ({{email}})

        <form class="delete" action="/user/{{id}}/delete/confirm" method="post">
          <button type="submit" name="button">Delete</button>
        </form>
      </li>
    {{/each}}
    </ul>

    <a href="/user-new">Add New User</a>

  </body>
</html>
